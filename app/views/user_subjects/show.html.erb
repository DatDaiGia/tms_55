<% provide :title, t(".my_subject") %>
<div class = "page-header">
  <h2><%= t ".my_subject" %></h2>
</div>
<hr>
<table class = "table table-striped">
  <thead>
    <tr>
      <th><%= t ".name" %></th>
      <th><%= t ".start_date" %></th>
      <th><%= t ".end_date"%></th>
    </tr>
  </thead>
  <tbody>
    <% @subjects.each do |subject|%>
      <tr>
        <td><%= subject.name %></td>
        <td><%= subject.start_date.strftime t ".datetime" %></td>
        <td><%= subject.end_date.strftime t ".datetime" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
