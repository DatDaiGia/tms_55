<% provide :title, t("supervisor.users.all_users") %>
<div class = "page-header">
  <h2><%= t "supervisor.users.all_trainees" %></h2>
</div>
<hr>
<table class = "table table-striped">
  <thead>
    <tr>
      <th><%= t "name" %></th>
      <th><%= t "email" %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
